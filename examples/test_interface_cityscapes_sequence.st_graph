name: test_interface_cityscapes_sequence
agents: 2
neuron_pools:
    image:
        shape: [3, 256, 128]
        tags: [sensor]
    label:
        shape: [19, 256, 128]
        tags: [sensor]
    class:
        shape: [3, 256, 128]
        tags: [sensor]
    yaw:
        shape: [1, 1, 1]
        tags: [sensor]
    speed:
        shape: [1, 1, 1]
        tags: [sensor]
    mask:
        shape: [1, 256, 128]
        tags: [sensor]
synapse_pools: {}
plasticities: {}
interfaces:
    cityscapes:
        type: cityscapes_sequence
        processes: 8
        samples: 24
        in: []
        out: [css_image, css_class, css_label, css_mask, css_speed, css_yaw]
        remap:
            css_image: image
            css_class: class
            css_label: label
            css_mask: mask
            css_speed: speed
            css_yaw: yaw
        label_path: /local/data/datasets/cityscapes/gtFine_trainvaltest/gtFine/
        image_path: /local/data/datasets/cityscapes/leftImg8bit_sequence/
        vehicle: /local/data/datasets/cityscapes/vehicle_sequence.zip
        min_duration: 24
        max_duration: 28