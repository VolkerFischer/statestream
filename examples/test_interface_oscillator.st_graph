name: test_interface_oscillator
agents: 16
neuron_pools:
    osci_sin:
        shape: [4, 32, 32]
        tags: [sensor]
synapse_pools: {}
plasticities: {}
interfaces:
    rnd_if_sin:
        type: oscillator
        in: []
        out: [osci]
        remap:
            osci: osci_sin
        osci_type: sin
        amlitude: 3.141
        frequency: 0.1
        phase: 0.0
